{
  "name": "@dreamstream/source",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Act-Aks",
    "url": "https://github.com/Act-Aks/dreamstream"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "@biomejs/biome": "2.3.11",
      "@semantic-release/changelog": "6.0.3",
      "@semantic-release/git": "10.0.1",
      "axios": "1.13.2",
      "cheerio": "1.0.0-rc.12",
      "dotenv": "17.2.3",
      "husky": "9.1.7",
      "lint-staged": "16.2.7",
      "node-html-parser": "7.0.2",
      "semantic-release": "25.0.2",
      "tsup": "8.5.1",
      "tsx": "4.21.0",
      "turbo": "2.7.4",
      "typescript": "5.9.3",
      "ultracite": "7.0.11",
      "zod": "4.3.5"
    },
    "catalogs": {
      "types": {
        "@types/node": "24.10.4",
        "@types/react": "19.2.8"
      }
    }
  },
  "type": "module",
  "scripts": {
    "build": "bun x turbo run build",
    "cq:check": "bun x turbo run cq:check",
    "cq:fix": "bun x turbo run cq:fix",
    "cq:types": "bun x turbo run cq:types",
    "dev:native": "bun x turbo run --filter @dreamstream/native dev",
    "dev:web": "bun x turbo run --filter @dreamstream/web dev",
    "dev": "bun x turbo run dev",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=24"
  },
  "packageManager": "bun@1.3.5",
  "dependencies": {
    "dotenv": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@semantic-release/changelog": "catalog:",
    "@semantic-release/git": "catalog:",
    "@types/node": "catalog:types",
    "@types/react": "catalog:types",
    "husky": "catalog:",
    "lint-staged": "catalog:",
    "semantic-release": "catalog:",
    "tsx": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "ultracite": "catalog:"
  }
}
