{
  "name": "@dreamstream/providers",
  "version": "1.0.0",
  "description": "Holds all providers for dreamstream project",
  "author": {
    "name": "Act-Aks",
    "url": "https://github.com/Act-Aks"
  },
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Act-Aks/dreamstream.git"
  },
  "bugs": {
    "url": "https://github.com/Act-Aks/dreamstream/issues"
  },
  "homepage": "https://github.com/Act-Aks/dreamstream#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "tsup",
    "cq:check": "bun ultracite check",
    "cq:fix": "bun ultracite fix",
    "cq:types": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "@dreamstream/core": "workspace:*",
    "@dreamstream/extractors": "workspace:*",
    "axios": "catalog:",
    "cheerio": "catalog:",
    "node-html-parser": "catalog:"
  },
  "devDependencies": {
    "tsup": "catalog:"
  }
}
