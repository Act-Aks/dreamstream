{
  "name": "@dreamstream/core",
  "version": "1.0.0",
  "description": "Core types and utilities for dreamstream project",
  "author": {
    "name": "Act-Aks",
    "url": "https://github.com/Act-Aks"
  },
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./axiosClient": {
      "types": "./dist/axiosClient/index.d.ts",
      "import": "./dist/axiosClient/index.js"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.js"
    },
    "./generics": {
      "types": "./dist/generics/index.d.ts",
      "import": "./dist/generics/index.js"
    },
    "./typeguards": {
      "types": "./dist/typeguards/index.d.ts",
      "import": "./dist/typeguards/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Act-Aks/dreamstream.git"
  },
  "bugs": {
    "url": "https://github.com/Act-Aks/dreamstream/issues"
  },
  "homepage": "https://github.com/Act-Aks/dreamstream#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "tsup",
    "cq:check": "bun ultracite check",
    "cq:fix": "bun ultracite fix",
    "cq:types": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "axios": "catalog:"
  },
  "devDependencies": {
    "tsup": "catalog:"
  }
}
