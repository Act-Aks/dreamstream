{
  "name": "@dreamstream/core",
  "version": "1.0.0",
  "description": "Core types and utilities for dreamstream project",
  "author": "Act-Aks",
  "license": "MIT",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./axiosClient": {
      "import": "./dist/axiosClient/index.js",
      "require": "./dist/axiosClient/index.cjs",
      "types": "./dist/axiosClient/index.d.ts"
    },
    "./constants": {
      "import": "./dist/constants/index.js",
      "require": "./dist/constants/index.cjs",
      "types": "./dist/constants/index.d.ts"
    },
    "./generics": {
      "import": "./dist/generics/index.js",
      "require": "./dist/generics/index.cjs",
      "types": "./dist/generics/index.d.ts"
    },
    "./typeguards": {
      "import": "./dist/typeguards/index.js",
      "require": "./dist/typeguards/index.cjs",
      "types": "./dist/typeguards/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "cq:check": "bun ultracite check",
    "cq:fix": "bun ultracite fix",
    "cq:types": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "axios": "catalog:"
  },
  "devDependencies": {
    "tsup": "catalog:"
  }
}
