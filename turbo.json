{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "check": {
      "dependsOn": ["^cq:check"]
    },
    "fix": {
      "dependsOn": ["^cq:fix"]
    },
    "types": {
      "dependsOn": ["^cq:types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
