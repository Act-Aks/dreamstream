{
  "name": "@dreamstream/app",
  "main": "expo-router/entry",
  "version": "1.7.2",
  "scripts": {
    "dev": "bun expo start",
    "android": "bun expo run:android",
    "ios": "bun expo run:ios",
    "web": "bun expo start --web",
    "format": "bun ultracite fix",
    "lint": "bun ultracite check",
    "prebuild": "bun expo prebuild",
    "update": "bun update --latest && bun i",
    "test": "jest",
    "testDebug": "jest -o --watch --coverage=false",
    "updateSnapshots": "jest -u --coverage=false",
    "release": "semantic-release",
    "version:set": "bun scripts/update-version.ts",
    "build:android": "bun eas build --platform android --profile production",
    "build:android:release": "bun eas build --platform android --profile production --local",
    "check-device-arch": "node scripts/check-device-arch.js",
    "test-apk-build": "bash scripts/test-apk-build.sh",
    "build-variants": "bash scripts/build-apk-variants.sh",
    "generate-qr": "bash scripts/generate-qr-codes.sh",
    "calc-sizes": "bash scripts/calculate-apk-sizes.sh"
  },
  "dependencies": {
    "@dreamstream/common": "workspace:*",
    "@dreamstream/providers": "workspace:*",
    "@expo/vector-icons": "^15.0.2",
    "expo": "54.0.13",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "18.0.9",
    "expo-dev-client": "6.0.15",
    "expo-haptics": "15.0.7",
    "expo-image": "3.0.9",
    "expo-linking": "8.0.8",
    "expo-router": "6.0.12",
    "expo-splash-screen": "31.0.10",
    "expo-status-bar": "3.0.8",
    "expo-system-ui": "6.0.7",
    "expo-video": "3.0.11",
    "react": "19.2.0",
    "react-native": "0.82.0",
    "react-native-gesture-handler": "2.28.0",
    "react-native-mmkv": "3.3.3",
    "react-native-reanimated": "4.1.3",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "4.16.0",
    "react-native-worklets": "0.6.1",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@expo/cli": "54.0.11",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.6",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@testing-library/react-native": "13.3.3",
    "@types/jest": "30.0.0",
    "@types/node": "24.7.2",
    "@types/react": "19.2.2",
    "eas-cli": "16.23.0",
    "jest": "30.2.0",
    "jest-expo": "54.0.12",
    "semantic-release": "24.2.9",
    "typescript": "5.9.3"
  },
  "private": true
}
