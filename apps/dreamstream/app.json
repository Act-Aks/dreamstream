{
  "expo": {
    "name": "DreamStream",
    "slug": "DreamStream",
    "version": "1.4.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dreamstream",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.actaks.dreamstream"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": true,
      "package": "com.actaks.dreamstream",
      "versionCode": 7
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "reactNativeArchitectures": [
              "arm64-v8a",
              "armeabi-v7a",
              "x86",
              "x86_64"
            ]
          }
        }
      ],
      [
        "./mods/plugins/withAbiSplits.js",
        {
          "abiFilters": ["arm64-v8a", "armeabi-v7a", "x86", "x86_64"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8f9b2549-8352-42ef-a108-78c49aa6fdf7"
      }
    },
    "owner": "act-aks"
  }
}
